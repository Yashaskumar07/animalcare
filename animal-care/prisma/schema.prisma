generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Pet {
  id          String   @id @default(uuid())
  name        String
  species     String
  age         Int
  imageUrl    String
  description String
  status      String
  createdAt   DateTime @default(now())
}
model VetAppointment {
  id         Int      @id @default(autoincrement())
  petName    String
  ownerName  String
  email      String
  species    String
  date       DateTime
  time       String
  reason     String
  createdAt  DateTime @default(now())
}
model Contact {
  id      String   @id @default(uuid())
  name    String
  email   String
  message String
  createdAt DateTime @default(now())
}
